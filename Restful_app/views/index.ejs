<% include ./partials/header %>
<div class="container main">
    <% blogs.forEach(function(blog){ %>
        <div>
           <h1><%=blog.title %></h1> 
            <img src="<%= blog.image %>" > 
            <p><%=blog.body.substring(0, 200) %>...</p>
            <%=blog.created %>
            <a href="/blogs/<%=blog._id%>">Read More...</a>
        </div>
    <% }); %>
</div>

<% include ./partials/footer %>